                        span.navbar-toggler-icon
                #navcol-1.collapse.navbar-collapse
                    ul.nav.navbar-nav
                        li.nav-item(role='presentation')
                            a.nav-link(href='#') Lorem 
                        li.nav-item(role='presentation')
                            a.nav-link(href='#') Ipsum 
                    form.form-inline.mb-0
                        input.form-control.mr-sm-2(type='search', placeholder='Search', aria-label='Search')
                        button.btn.btn-outline-primary.my-2.my-sm-0(type='submit') Search
                    ul.nav.navbar-nav.ml-auto
                        li.nav-item(role='presentation')
                            a.nav-link(href='#') Đăng ký
                        li.nav-item(role='presentation')
                            a.nav-link(href='#') Đăng nhập
        div
            header
                h1 BookSeller
            nav
                ul
                    a(href='')
                        li
