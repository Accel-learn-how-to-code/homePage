        title HOME
    body
        .navbar.navbar-light.navbar-expand-md
            .container-fluid
                div
                    a.navbar-brand.navbar-brand-centered(href='#')
                        img(style='max-height:50px;', src="../assets/img/BS.png")
                    button.navbar-toggler(data-toggle='collapse', data-target='#navcol-1')
